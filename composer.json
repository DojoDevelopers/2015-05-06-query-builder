{
    "name": "dojodevelopers/skeleton",
    "description": "Dojo Skeleton",
    "keywords": [
        "dojo",
        "developers"
    ],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
